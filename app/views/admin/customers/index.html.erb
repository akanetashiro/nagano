<h3>会員一覧</h3>

<table class="table">
  <thead>
    <tr>
      <th scope="col">会員ID</th>
      <th scope="col">氏名</th>
      <th scope="col">メールアドレス</th>
      <th scope="col">ステータス</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <% @customers.each do |customer| %>
      <td><%= customers.id %></td>
      <td><%= customers.last_name %><%= customers.first_name %></td>
      <td><%= customers.email %></td>
      <td><%= customers.is_deleted %></td>
    </tr>
    
  </tbody>
</table>
